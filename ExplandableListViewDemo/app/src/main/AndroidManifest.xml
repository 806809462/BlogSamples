<?xml version="1.0" encoding="utf-8"?>
<manifest package="org.lynxz.explandablelistviewdemo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".PinnedHeaderActivity">
        </activity>
        <activity
            android:name=".BasicExpandableListView"
            android:screenOrientation="portrait"/>
        <activity android:name=".AnimatedActivity">
        </activity>
        <activity
            android:name=".ThreeLevelActivity"
            android:label="@string/title_activity_three_level"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>